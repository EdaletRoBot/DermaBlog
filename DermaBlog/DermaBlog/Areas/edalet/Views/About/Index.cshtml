@model List<About>

@{
    ViewData["About"] = "Index";
}

@foreach (About item in Model)
{
    <div class="col-12">
        <div class="col-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">About Details</h4>
                    <div class="form-group">
                        <label for="exampleInputName1">Name</label>
                        <div>@item.Title</div>
                    </div>
                    <div class="form-group">
                        <label>Ourstory</label>
                        <div>@item.ourstory</div>
                    </div>
                    <div class="form-group">
                        <label for="exampleTextarea1">Ourvision</label>
                        <div>@item.ourvision</div>
                    </div>

                    <center><a asp-action="Update"><button type="button" class="btn btn-info btn-fw">Edit</button></a></center>
                </div>
            </div>
        </div>
    </div>
}




@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
