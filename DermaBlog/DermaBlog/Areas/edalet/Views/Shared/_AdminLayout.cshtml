        <!-- partial -->
        <div class="main-panel">
            <div class="content-wrapper">
        @RenderBody()
            <footer class="footer">
                <div class="container-fluid d-flex justify-content-between">
                        <span class="float-none float-sm-end mt-1 mt-sm-0 text-end"> Copyright © <a href="https://www.edaletbalaev.com" target="_blank">edaletbalaev.com</span>
                </div>
            </footer>
            <!-- partial -->
        </div>
        <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Admin/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
